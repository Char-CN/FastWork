#!/bin/bash
#####################################
# 1. edit ~/.go                     #
# 2. command : go $name             #
#####################################
s_p=`dirname $0`
source ${s_p}/gocommonsfunc
if [ "$1" == "" ]; then
    echo "    usage : go \$name"
    echo "      please input paramter ..."
    exit -1
elif [[ "$1" == "config" || "$1" == "conf" ]]; then
    echo -e "$(echo_go)"
    exit 0
fi
arr=$(get_go $1)
if [ $? != 0 ]; then
    echo "    $arr"
    exit 0
fi
expect ${s_p}/goex $arr
exit 0
